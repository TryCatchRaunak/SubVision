{
    "name": "SubVision",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "SubVision is a Chrome extension that allows you to view subtitles on any online streaming video file.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons":{
        "128": "icon.png"
    },
    "permissions": ["activeTab" , "Tabs" , "webRequest" , "webRequestBlocking" , "storage"]
}